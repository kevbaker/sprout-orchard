<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-/Apple/DTD PLIST 1.0/EN" "http:/www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.pivotallabs.auto_set_hostname</string>
	<key>LimitLoadToSessionType</key>
	<string>LoginWindow</string>
	<key>ProgramArguments</key>
	<array>
          <string>/usr/sbin/auto_set_hostname.rb</string>
	</array>
        <key>KeepAlive</key>
        <false/>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
        <string>/tmp/com.pivotallabs.auto_set_hostname.stderr</string>
	<key>StandardOutPath</key>
        <string>/tmp/com.pivotallabs.auto_set_hostname.stdout</string>
</dict>
</plist>
